    "buy": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "bedrooms": {
      "type": "integer",
      "required": true,
      "unique": false
    },
    "bathrooms": {
      "type": "integer",
      "required": true
    },
    "reception_rooms": {
      "type": "integer",
      "required": true
    },
    "deposit": {
      "type": "decimal",
      "required": true
    },
    "cost": {
      "type": "decimal",
      "required": true
    },
    "property_type": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "slider_images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
